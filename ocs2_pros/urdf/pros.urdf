<?xml version="1.0" ?>
<robot name="robot">
<!-- 颜色 -->
    <material name="black">
        <color rgba="0.0 0.0 0.0 1.0"/>
    </material>
    <material name="blue">
        <color rgba="0.0 0.0 0.8 1.0"/>
    </material>
    <material name="green">
        <color rgba="0.0 0.8 0.0 1.0"/>
    </material>
    <material name="grey">
        <color rgba="0.2 0.2 0.2 1.0"/>
    </material>
    <material name="silver">
        <color rgba="0.9137254901960784 0.9137254901960784 0.8470588235294118 1.0"/>
    </material>
    <material name="orange">
        <color rgba="1.0 0.4235294117647059 0.0392156862745098 1.0"/>
    </material>
    <material name="brown">
        <color rgba="0.8705882352941177 0.8117647058823529 0.7647058823529411 1.0"/>
    </material>
    <material name="red">
        <color rgba="0.8 0.0 0.0 1.0"/>
    </material>
    <material name="white">
        <color rgba="1.0 1.0 1.0 1.0"/>
    </material>

    <!-- ros_control plugin -->
    <!-- 使用legged_control的HWSim框架 -->
    <gazebo>
        <plugin filename="liblegged_hw_sim.so" name="gazebo_ros_control">
        <robotNamespace>/</robotNamespace>
        <robotParam>legged_robot_description</robotParam>
        <robotSimType>legged_gazebo/LeggedHWSim</robotSimType>
        </plugin>
    </gazebo>
    <gazebo>
        <plugin filename="libgazebo_ros_p3d.so" name="p3d_base_controller">
            <alwaysOn>true</alwaysOn>
            <updateRate>1000.0</updateRate>
            <bodyName>base</bodyName>
            <topicName>ground_truth/state</topicName>
            <gaussianNoise>0</gaussianNoise>
            <frameName>world</frameName>
            <xyzOffsets>0 0 0</xyzOffsets>
            <rpyOffsets>0 0 0</rpyOffsets>
        </plugin>
    </gazebo>
    <link name="base">
        <visual>
            <origin rpy="0 1.57 0" xyz="0 0 0"/>
            <geometry>
                <box size="0.267 0.194 0.114"/>
            </geometry>
        </visual>
        <collision>
            <origin rpy="0 1.57 0" xyz="0 0 0"/>
            <geometry>
                <box size="0.267 0.194 0.114"/>
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 1.57 0" xyz="0.0 0.0041 -0.0005"/>
            <mass value="200"/>
            <inertia ixx="0.8438667" ixy="-0.0000001" ixz="-0.0000001" 
            iyy="1.40475" iyz="-0.0000001" izz="1.8154167"/>
        </inertial>
    </link>
    <!-- Imu is fixed to the base link -->
    <joint name="unitree_imu_joint" type="fixed">
    <origin rpy="0. 0. 0." xyz="0. 0. 0."/>
    <parent link="base"/>
    <child link="unitree_imu"/>
    </joint>
    <!-- Imu link -->
    <link name="unitree_imu">
    <inertial>
      <mass value="0.01"/>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <inertia ixx="0.000001" ixy="0" ixz="0" iyy="0.000001" iyz="0" izz="0.000001"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.015 0.015 0.004"/>
      </geometry>
    </visual>
    <material name="orange">
      <color rgba="255 108 10 255"/>
    </material>
    </link>
    <gazebo reference="unitree_imu">
        <material>Gazebo/Orange</material>
    </gazebo>
    <gazebo reference="unitree_imu_joint">
        <disableFixedJointLumping>true</disableFixedJointLumping>
    </gazebo>

    <joint name="com_slide_x" type="prismatic">
        <origin rpy="0 0 0" xyz="0.4 0 1.2"/>
        <parent link="base"/>
        <child link="com_x"/>
        <axis xyz="1 0 0"/>
        <dynamics damping="10" friction="10"/>
        <limit effort="100" lower="-5" upper="5" velocity="5"/>
    </joint>
    <link name="com_x">
        <visual>
            <origin rpy="1.5707963267948966 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.04" radius="0.046"/>
            </geometry>
            <material name="orange"/>
        </visual>
        <!-- <collision>
            <origin rpy="1.5707963267948966 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.04" radius="0.046"/>
            </geometry>
        </collision> -->
        <inertial>
            <origin rpy="1.5707963267948966 0 0" xyz="0 0 0"/>
                <mass value="2"/>
            <inertia ixx="0.0013246666666666667" ixy="1e-06" ixz="-1e-06" 
            iyy="0.0013246666666666667" iyz="1e-06" izz="0.002116"/>
        </inertial>
    </link>
    <gazebo reference="com_x">
        <mu1>0.2</mu1>
        <mu2>0.2</mu2>
        <material>Gazebo/DarkGrey</material>
    </gazebo>
    <transmission name="slide_x_tran">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="com_slide_x">
            <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
        </joint>
        <actuator name="com_slidex_motor">
            <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>

    <joint name="com_slide_z" type="prismatic">
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <parent link="com_x"/>
        <child link="com_z"/>
        <axis xyz="0 0 1"/>
        <dynamics damping="10" friction="10"/>
        <limit effort="100" lower="-0.2" upper="0.2" velocity="5"/>
    </joint>
    <link name="com_z">
        <visual>
            <origin rpy="1.5707963267948966 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.04" radius="0.046"/>
            </geometry>
            <material name="orange"/>
        </visual>
        <!-- <collision>
            <origin rpy="1.5707963267948966 0 0" xyz="0 0 0"/>
            <geometry>
                <cylinder length="0.04" radius="0.046"/>
            </geometry>
        </collision> -->
        <inertial>
            <origin rpy="1.5707963267948966 0 0" xyz="0 0 0"/>
                <mass value="2"/>
            <inertia ixx="0.0013246666666666667" ixy="1e-06" ixz="-1e-06" 
            iyy="0.0013246666666666667" iyz="1e-06" izz="0.002116"/>
        </inertial>
    </link>
    <gazebo reference="com_z">
        <mu1>0.2</mu1>
        <mu2>0.2</mu2>
        <material>Gazebo/Write</material>
    </gazebo>
    <transmission name="slide_z_tran">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="com_slide_z">
            <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
        </joint>
        <actuator name="com_slidez_motor">
            <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>

    <joint name="hip" type="revolute">
        <origin rpy="0 0 0" xyz="0 0 0"/>
        <parent link="com_z"/>
        <child link="thigh"/>
        <axis xyz="0 1 0"/>
        <dynamics damping="0" friction="0"/>
        <limit effort="100" lower="-3.14" upper="3.14" velocity="21"/>
    </joint>
    <link name="thigh">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 -0.22"/>
            <geometry>
                <cylinder length="0.36" radius="0.046"/>
            </geometry>
            <material name="orange"/>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 -0.22"/>
            <geometry>
                <cylinder length="0.36" radius="0.02"/>
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
            <mass value="2.0"/>
            <inertia ixx="0.0218" ixy="-1e-06" ixz="-1e-06" 
                    iyy="0.0218" iyz="-1e-06" izz="0.0004"/>
        </inertial>
    </link>
    <gazebo reference="thigh">
        <mu1>0.2</mu1>
        <mu2>0.2</mu2>
        <material>Gazebo/Write</material>
    </gazebo>
    <transmission name="hip_tran">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="hip">
            <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
        </joint>
        <actuator name="hip_motor">
            <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>

    <joint name="knee" type="revolute">
        <origin rpy="0 0 0" xyz="0 0 -0.38"/>
        <parent link="thigh"/>
        <child link="shank"/>
        <axis xyz="0 1 0"/>
        <dynamics damping="0" friction="0"/>
        <limit effort="100" lower="-1.5" upper="1.5" velocity="21"/>
    </joint>

    <link name="shank">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 -0.22"/>
            <geometry>
                <cylinder length="0.36" radius="0.046"/>
            </geometry>
            <material name="orange"/>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 -0.22"/>
            <geometry>
                <cylinder length="0.36" radius="0.02"/>
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
            <mass value="2.0"/>
            <inertia ixx="0.0218" ixy="-1e-06" ixz="-1e-06" 
                    iyy="0.0218" iyz="-1e-06" izz="0.0004"/>
        </inertial>
    </link>
    <gazebo reference="shank">
        <mu1>0.2</mu1>
        <mu2>0.2</mu2>
        <material>Gazebo/Gray</material>
    </gazebo>

    <transmission name="knee_tran">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="knee">
            <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
        </joint>
        <actuator name="knee_motor">
            <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>

    <joint name="ankle" type="revolute">
        <origin rpy="0 0 0" xyz="0 0 -0.38"/>
        <parent link="shank"/>
        <child link="foot"/>
        <axis xyz="0 1 0"/>
        <dynamics damping="0" friction="0"/>
        <limit effort="100" lower="-1.5" upper="1.5" velocity="21"/>
    </joint>

    <link name="foot">
        <visual>
            <origin rpy="0 0 0" xyz="0 0 -0.14"/>
            <geometry>
                <cylinder length="0.22" radius="0.046"/>
            </geometry>
            <material name="orange"/>
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 -0.14"/>
            <geometry>
                <cylinder length="0.22" radius="0.02"/>
            </geometry>
        </collision>
        <inertial>
            <origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
            <mass value="1.0"/>
            <inertia ixx="0.004133333333333333" ixy="-1e-07" ixz="-1e-07" 
                    iyy="0.004133333333333333" iyz="-1e-07" izz="0.0002"/>
        </inertial>
    </link>
    <gazebo reference="foot">
        <mu1>0.2</mu1>
        <mu2>0.2</mu2>
        <material>Gazebo/Gray</material>
    </gazebo>

    <transmission name="ankle_tran">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="ankle">
            <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
        </joint>
        <actuator name="ankle_motor">
            <hardwareInterface>hardware_interface/EffortJointInterface</hardwareInterface>
            <mechanicalReduction>1</mechanicalReduction>
        </actuator>
    </transmission>

</robot>